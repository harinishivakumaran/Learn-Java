package exceptHandle;

public class CheckedExceptionRunner {

	public static void main(String[] args) {
		try {
			someMethod();
			Thread.sleep(2000);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		
		
	}

	private static void someMethod() throws InterruptedException {
		
		Thread.sleep(2000);
		
	}

}
