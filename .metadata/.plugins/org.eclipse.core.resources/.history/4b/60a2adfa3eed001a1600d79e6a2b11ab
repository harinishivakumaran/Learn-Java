package exceptHandle;

import java.util.Scanner;

public class FinallyRunner {

	public static void main(String[] args) {
		try {
			Scanner sc=new Scanner(System.in);
			int[] a= {12,3,4,23};
			int b=a[5];
			System.out.println("Before sc close");
			sc.close();
		} catch(Exception e) {
			e.printStackTrace();
		}
		System.out.println("Finish");
	}

}
